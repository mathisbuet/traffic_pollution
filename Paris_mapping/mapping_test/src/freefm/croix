pt0p0 = [0.5,4.5]; pt0p1 = [0.5,5.5]; pt0p2 = [1.5,5.5]; pt0p3 = [1.5,4.5]; pt1p0 = [5.5,4.5]; pt1p1 = [5.5,5.5]; pt1p2 = [6.5,5.5]; pt1p3 = [6.5,4.5]; pt2p0 = [11.5,4.5]; pt2p1 = [11.5,5.5]; pt2p2 = [12.5,5.5]; pt2p3 = [12.5,4.5]; pt3p0 = [5.5,0.5]; pt3p1 = [5.5,1.5]; pt3p2 = [6.5,1.5]; pt3p3 = [6.5,0.5]; pt4p0 = [5.5,11.5]; pt4p1 = [5.5,12.5]; pt4p2 = [6.5,12.5]; pt4p3 = [6.5,11.5]; 

